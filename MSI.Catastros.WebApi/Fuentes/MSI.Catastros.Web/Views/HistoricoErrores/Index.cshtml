@model MSI.Catastros.Web.Funcionalidad.HistoricoErrores.FiltrarHistoricoErroresViewModel

@{
    ViewBag.Title = "Historico Errores";
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading" style="overflow: hidden">
                <div class="row">
                    <div class="col-md-6 col-xs-12"><strong>Historico Errores</strong></div>
                    <div class="col-md-6 col-xs-12">
                        <span>
                            @Ajax.ActionLink(" Salir", "Registrar", "ManzanaUrbana", new { }
                            , new AjaxOptions{HttpMethod = "Get",InsertionMode = InsertionMode.Replace,UpdateTargetId = "modalManzanaUrbana"}
                            , new {@class = "btn btn-green mr-2 px-4",style = "float:right",@type = "button",data_toggle = "modal",data_target = "#modalManzanaUrbana"})
                        </span>
                        <span>
                            @Ajax.ActionLink(" Historico Errores", "Registrar", "ManzanaUrbana", new { }
                            , new AjaxOptions{HttpMethod = "Get",InsertionMode = InsertionMode.Replace,UpdateTargetId = "modalManzanaUrbana"}
                            ,new{ @class = "btn btn-green mr-2 px-3",style = "float:right",@type = "button",data_toggle = "modal",data_target = "#modalManzanaUrbana"})
                        </span>
                    </div>
                </div>
            </div>
            <div class="panel-body">
                <div class="">

                    @Html.ValidationSummary(true)
                    <div class="row">
                        <div class="col-sm-3 px-3">
                            <div class="form-group">
                                @Html.LabelFor(model => model.NroTramiteDocumentario, htmlAttributes: new { @class = "control-label" })
                                @Html.TextBoxFor(model => model.NroTramiteDocumentario, new { @class = "form-control" })
                                @Html.ValidationMessageFor(model => model.NroTramiteDocumentario)
                            </div>
                        </div>
                        <div class="col-sm-3 px-3">
                            <div class="form-group">
                                @Html.LabelFor(model => model.NroSolicitud, htmlAttributes: new { @class = "control-label" })
                                <div class="input-group">
                                    @Html.TextBoxFor(model => model.NroSolicitud, new { @class = "form-control" })
                                    <span class="input-group-btn">
                                        <button class="btn btn-github" type="button">
                                            <i class="fa fa-search" aria-hidden="true"></i>
                                        </button>
                                    </span>
                                </div><!-- /input-group -->
                                @Html.ValidationMessageFor(model => model.NroSolicitud)
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-md-12 px-3">
                            <table class="table table-striped table-bordered table-hover">
                                <thead>
                                    <tr>
                                        <th>Dist</th>
                                        <th>Sec</th>
                                        <th>Mz</th>
                                        <th>Lote</th>
                                        <th>Ed</th>
                                        <th>Entr</th>
                                        <th>Piso</th>
                                        <th>Dpto</th>
                                        <th>Nro. Revisión</th>
                                        <th>Fecha Revisión</th>
                                        <th>Tipo Procedimiento</th>
                                        <th>Responsable Actual</th>
                                        <th>Tipo Error</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr><td></td><td></td><td></td><td></td><td></td><td></td><td></td><td></td><td></td><td></td><td></td><td></td><td></td></tr>
                                    <tr><td></td><td></td><td></td><td></td><td></td><td></td><td></td><td></td><td></td><td></td><td></td><td></td><td></td></tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@section datatables {
    <script>
        $(document).ready(function () {
            
        });
    </script>
}








